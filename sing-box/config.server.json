{
    "log": {
        "disabled": false,
        "level": "info",
        "timestamp": true
    },
    "inbounds": [
        {
            "type": "trojan",
            "tag": "trojan-in",
            "listen": "::",
            "listen_port": 9964,
            "tcp_fast_open": true,
            "udp_fragment": true,
            "sniff": true,
            "sniff_override_destination": true,
            "sniff_timeout": "300ms",
            "udp_timeout": 300,
            "proxy_protocol": true,
            "proxy_protocol_accept_no_header": true,
            "users": [
                {
                    "name": "USERNAME",
                    "password": "PASSWORD"
                }
            ],
            "tls": {
                "enabled": true,
                "server_name": "EXAMPLE.COM",
                "alpn": [
                    "http/1.1",
                    "h2"
                ],
                "acme": {
                    "domain": [
                        "EXAMPLE.COM"
                    ],
                    "data_directory": "./cert/",
                    "email": "EMAIL",
                    "provider": "letsencrypt",
                    "alternative_http_port": 9963
                }
            },
            "fallback": {
                "server": "127.0.0.1",
                "server_port": 8080
            }
        },
        {
            "type": "hysteria",
            "tag": "hysteria-in",
            "listen_port": 9965,
            "up_mbps": 100,
            "down_mbps": 100,
            "obfs": "OBFS",
            "users": [
                {
                    "name": "USERNAME",
                    "auth_str": "AUTH_STR"
                }
            ],
            "tls": {
                "enabled": true,
                "server_name": "EXAMPLE.COM",
                "alpn": [
                    "http/1.1",
                    "h2"
                ],
                "acme": {
                    "domain": [
                        "EXAMPLE.COM"
                    ],
                    "data_directory": "./cert/",
                    "email": "EMAIL",
                    "provider": "letsencrypt"
                }
            }
        }
    ],
    "dns": {
        "servers": [
            {
                "tag": "local",
                "address": "8.8.8.8"
            },
            {
                "tag": "google",
                "address": "tls://8.8.8.8"
            },
            {
                "tag": "cloudflare-doh",
                "address": "https://cloudflare-dns.com/dns-query",
                "address_resolver": "local"
            }
        ],
        "strategy": "ipv4_only"
    },
    "route": {
        "final": "direct-out",
        "auto_detect_interface": true,
        "rules": []
    },
    "outbounds": [
        {
            "type": "direct",
            "tag": "direct-out"
        },
        {
            "type": "block",
            "tag": "block-out"
        }
    ]
}