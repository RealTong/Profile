#!name=Surge Panel 
#!desc=Surge Panel 包含Surge连接信息, 流量统计信息, 网络详情, Netflix解锁情况, YoutubePremium解锁情况, DisneyPlus解锁情况 

[Script]
# Surge Pro
SurgePro_ReloadProfile = type=generic,timeout=10,script-path=https://raw.githubusercontent.com/fishingworld/something/main/PanelScripts/surgepro_reloadprofile.js,argument=icon=crown.fill&color=#f6c970

# 流量统计 点击以切换网络界面
TrafficStatistics = type=generic,timeout=10,script-path=https://raw.githubusercontent.com/fishingworld/something/main/PanelScripts/trafficstatistics.js,argument=icon=arrow.up.arrow.down.circle&color=#5d84f8

# 网络信息
net-info-panel = script-path=https://raw.githubusercontent.com/Peng-YM/QuanX/master/Tools/Panels/NetInfo/net-info-panel.js,type=generic

# 网络详情 group= 代理策略组名称
NET_info = type=generic,timeout=10,script-path=https://raw.githubusercontent.com/fishingworld/something/main/PanelScripts/net_info.js,argument=icon=externaldrive.connected.to.line.below&color=#9a7ff7&group=🚀Proxy

# 机场信息 argument=url= encode后的机场订阅链接
Sub_info = type=generic,timeout=10,script-path=https://raw.githubusercontent.com/mieqq/mieqq/master/sub_info_panel.js,script-update-interval=0,argument=url=https%3A%2F%2Fservice-4b9cwk5j-1251130579.sh.apigw.tencentcs.com%2Fapi%2Fv1%2Fclient%2Fsubscribe%3Ftoken%3Dc7929ba8c51ccdc28a28c59494675171&reset_day=1&title=CJPC&icon=opticaldisc&color=#5AC8FA

# 策略组面板 可重复配置 注意修改相应字段   group= 需要显示的策略组名称
groupPanel = type=generic,timeout=10,script-path=https://raw.githubusercontent.com/fishingworld/something/main/groupPanel.js,argument=icon=network&color=#86abee&group=Master

# Netflix 检测
nf_check = type=generic,script-path=https://gist.githubusercontent.com/Hyseen/b06e911a41036ebc36acf04ddebe7b9a/raw/nf_check.js?version=1633074636264,argument=title=Netflix

# Disney Plus 检测
disney_check = type=generic,script-path=https://gist.githubusercontent.com/Hyseen/729fc4c3ac28c3f2e7c4a832d81780f3/raw/disney_check.js,argument=title=Disney+

# Youtube Premium 检测
youtube_premium_check = type=generic,script-path=https://gist.githubusercontent.com/Hyseen/5ae36a6a5cb5690b1f2bff4aa19c766f/raw/youtube_premium_check.js?version=1633090827446,argument=title=YouTube

[Panel]
# Surge Info
SurgePro_ReloadProfile = script-name=SurgePro_ReloadProfile,update-interval=1

# Traffic Info
TrafficStatistics = script-name=TrafficStatistics,update-interval=1

net-info-panel = title=网络状态,content=请刷新,style=info,script-name=net-info-panel

# 网络信息
NET_info = script-name=NET_info,update-interval=1

# Netflix 检测脚本
nf_check = script-name=nf_check,title=Netflix,content=请刷新,update-interval=1

youtube_premium_check = script-name=youtube_premium_check,title=YouTube Premium,update-interval=1

disney_check = script-name=disney_check,title=Disney+,style=alert,content=请刷新,update-interval=1800

# 机场订阅信息
Sub_info = script-name=Sub_info,update-interval=43200

# 策略组面板 可重复配置 注意修改相应字段
groupPanel = script-name=groupPanel,update-interval=5